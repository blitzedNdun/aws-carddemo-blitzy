  "validationRules": {
    "categoryCodeFormat": {
      "pattern": "^[0-9]{6}$",
      "description": "Category code must be exactly 6 digits"
    },
    "statusCodeValues": {
      "validValues": ["0000", "0001", "9999"],
      "descriptions": {
        "0000": "Active category",
        "0001": "Inactive category", 
        "9999": "Deprecated category"
      }
    },
    "balanceImpactTypes": {
      "validValues": ["DEBIT", "CREDIT", "HOLD", "REVERSAL", "ADJUSTMENT"],
      "descriptions": {
        "DEBIT": "Increases balance owed",
        "CREDIT": "Decreases balance owed",
        "HOLD": "Places temporary hold on available credit",
        "REVERSAL": "Reverses previous transaction impact",
        "ADJUSTMENT": "Manual balance correction"
      }
    },
    "balanceTypes": {
      "validValues": ["PURCHASE", "CASH", "INTEREST", "PAYMENT", "CREDIT", "AUTHORIZATION", "REFUND", "REVERSAL", "ADJUSTMENT"],
      "descriptions": {
        "PURCHASE": "Regular purchase transactions",
        "CASH": "Cash advance transactions",
        "INTEREST": "Interest charges",
        "PAYMENT": "Payment transactions",
        "CREDIT": "Credit transactions",
        "AUTHORIZATION": "Authorization holds",
        "REFUND": "Refund transactions",
        "REVERSAL": "Transaction reversals",
        "ADJUSTMENT": "Manual adjustments"
      }
    }
