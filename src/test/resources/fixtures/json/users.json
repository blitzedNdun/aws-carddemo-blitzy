  "COBOLConversionTestData": {
    "description": "Test data for validating COBOL to Spring Security conversion accuracy",
    "field_length_validation": [
      {
        "field": "SEC-USR-ID",
        "cobolDefinition": "PIC X(08)",
        "javaMapping": "String userId (max 8 chars)",
        "testValue": "ADMIN001",
        "validationRule": "exactly_8_chars_or_less"
      },
      {
        "field": "SEC-USR-FNAME",
        "cobolDefinition": "PIC X(20)",
        "javaMapping": "String firstName (max 20 chars)",
        "testValue": "System",
        "validationRule": "max_20_chars"
      },
      {
        "field": "SEC-USR-LNAME",
        "cobolDefinition": "PIC X(20)",
        "javaMapping": "String lastName (max 20 chars)",
        "testValue": "Administrator",
        "validationRule": "max_20_chars"
      },
      {
        "field": "SEC-USR-PWD",
        "cobolDefinition": "PIC X(08)",
        "javaMapping": "String passwordHash (VARCHAR(60) BCrypt)",
        "testValue": "$2a$10$N.zmdr9k7uOCQb376NoUnuTJ8iYqiSfFe5gtT4iUWIEzJ6.wYqCny",
        "validationRule": "bcrypt_hash_60_chars"
      },
      {
        "field": "SEC-USR-TYPE",
        "cobolDefinition": "PIC X(01)",
        "javaMapping": "String userType (VARCHAR(1))",
        "testValue": "A",
        "validationRule": "exactly_1_char"
      }
    ],
